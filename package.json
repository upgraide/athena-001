{
  "name": "athena-finance-agents",
  "version": "1.0.0",
  "description": "Secure AI-powered finance automation agents",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "deploy:infrastructure": "cd infrastructure/terraform && terraform apply",
    "deploy:services": "./scripts/deploy-services.sh",
    "deploy:staging": "gcloud builds submit --config=config/cloudbuild.yaml",
    "validate": "./scripts/testing/validate-deployment.sh"
  },
  "keywords": [
    "ai",
    "finance",
    "automation",
    "microservices",
    "security"
  ],
  "author": "Athena Finance Team",
  "license": "PRIVATE",
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/kms": "^4.0.0",
    "@google-cloud/logging-winston": "^6.0.1",
    "@google-cloud/secret-manager": "^5.0.0",
    "@google-cloud/storage": "^7.16.0",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
